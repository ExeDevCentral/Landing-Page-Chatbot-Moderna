{
  "name": "landing-page-chatbot",
  "version": "1.0.0",
  "description": "Landing page moderna con chatbot OpenAI y estructura preparada para n8n",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "dev:frontend": "webpack-dev-server --mode development --open",
    "build": "npm run build:frontend && npm run build:css",
    "build:frontend": "webpack --mode production",
    "build:css": "tailwindcss -i ./src/css/input.css -o ./public/css/style.css --watch",
    "seed": "node server/scripts/seed.js",
    "test": "jest",
    "lint": "eslint src/**/*.{ts,tsx} server/**/*.js",
    "lint:fix": "eslint src/**/*.{ts,tsx} server/**/*.js --fix",
    "format": "prettier --write src/**/*.{ts,tsx} server/**/*.js",
    "type-check": "tsc --noEmit"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  },
  "keywords": [
    "landing-page",
    "chatbot",
    "openai",
    "n8n",
    "restaurant",
    "tickets",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.0",
    "mongoose-paginate-v2": "^1.9.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "openai": "^4.20.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "socket.io": "^4.7.4",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^30.0.0",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/socket.io": "^3.0.1",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/testing-library__react": "^10.0.1",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "html-webpack-plugin": "^5.6.4",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.2",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.3.6",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
